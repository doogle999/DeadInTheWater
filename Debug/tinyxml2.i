	char* XMLAttribute::ParseDeep(char* p, bool processEntities)
	{
		
		p = _name.ParseName(p);
		if (!p || !*p) {
			return 0;
		}

		
		p = XMLUtil::SkipWhiteSpace(p);
		if (*p != '=') {
			return 0;
		}

		++p;	
		p = XMLUtil::SkipWhiteSpace(p);
		if (*p != '\"' && *p != '\'') {
			return 0;
		}

		char endTag[2] = { *p, 0 };
		++p;	

		p = _value.ParseText(p, endTag, processEntities ? StrPair::ATTRIBUTE_VALUE : StrPair::ATTRIBUTE_VALUE_LEAVE_ENTITIES);
		return p;
	}


