	void XMLNode::DeleteChildren()
	{
		while (_firstChild) {
			if ( !((void)0,(_lastChild))) { __debugbreak(); };
			DeleteChild(_firstChild);
		}
		_firstChild = _lastChild = 0;
	}


	void XMLNode::Unlink(XMLNode* child)
	{
		if ( !((void)0,(child))) { __debugbreak(); };
		if ( !((void)0,(child->_document == _document))) { __debugbreak(); };
		if ( !((void)0,(child->_parent == this))) { __debugbreak(); };
		if (child == _firstChild) {
			_firstChild = _firstChild->_next;
		}
		if (child == _lastChild) {
			_lastChild = _lastChild->_prev;
		}

		if (child->_prev) {
			child->_prev->_next = child->_next;
		}
		if (child->_next) {
			child->_next->_prev = child->_prev;
		}
		child->_parent = 0;
	}


	void XMLNode::DeleteChild(XMLNode* node)
	{
		if ( !((void)0,(node))) { __debugbreak(); };
		if ( !((void)0,(node->_document == _document))) { __debugbreak(); };
		if ( !((void)0,(node->_parent == this))) { __debugbreak(); };
		Unlink(node);
		DeleteNode(node);
	}


	XMLNode* XMLNode::InsertEndChild(XMLNode* addThis)
	{
		if ( !((void)0,(addThis))) { __debugbreak(); };
		if (addThis->_document != _document) {
			if ( !((void)0,(false))) { __debugbreak(); };
			return 0;
		}
		InsertChildPreamble(addThis);

		if (_lastChild) {
			if ( !((void)0,(_firstChild))) { __debugbreak(); };
			if ( !((void)0,(_lastChild->_next == 0))) { __debugbreak(); };
			_lastChild->_next = addThis;
			addThis->_prev = _lastChild;
			_lastChild = addThis;

			addThis->_next = 0;
		}
		else {
			if ( !((void)0,(_firstChild == 0))) { __debugbreak(); };
			_firstChild = _lastChild = addThis;

			addThis->_prev = 0;
			addThis->_next = 0;
		}
		addThis->_parent = this;
		return addThis;
	}


	XMLNode* XMLNode::InsertFirstChild(XMLNode* addThis)
	{
		if ( !((void)0,(addThis))) { __debugbreak(); };
		if (addThis->_document != _document) {
			if ( !((void)0,(false))) { __debugbreak(); };
			return 0;
		}
		InsertChildPreamble(addThis);

		if (_firstChild) {
			if ( !((void)0,(_lastChild))) { __debugbreak(); };
			if ( !((void)0,(_firstChild->_prev == 0))) { __debugbreak(); };

			_firstChild->_prev = addThis;
			addThis->_next = _firstChild;
			_firstChild = addThis;

			addThis->_prev = 0;
		}
		else {
			if ( !((void)0,(_lastChild == 0))) { __debugbreak(); };
			_firstChild = _lastChild = addThis;

			addThis->_prev = 0;
			addThis->_next = 0;
		}
		addThis->_parent = this;
		return addThis;
	}


	XMLNode* XMLNode::InsertAfterChild(XMLNode* afterThis, XMLNode* addThis)
	{
		if ( !((void)0,(addThis))) { __debugbreak(); };
		if (addThis->_document != _document) {
			if ( !((void)0,(false))) { __debugbreak(); };
			return 0;
		}

		if ( !((void)0,(afterThis))) { __debugbreak(); };

		if (afterThis->_parent != this) {
			if ( !((void)0,(false))) { __debugbreak(); };
			return 0;
		}

		if (afterThis->_next == 0) {
			
			return InsertEndChild(addThis);
		}
		InsertChildPreamble(addThis);
		addThis->_prev = afterThis;
		addThis->_next = afterThis->_next;
		afterThis->_next->_prev = addThis;
		afterThis->_next = addThis;
		addThis->_parent = this;
		return addThis;
	}




